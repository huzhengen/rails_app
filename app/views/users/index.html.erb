<% provide(:title, 'All users') %>
<h1>All users</h1>

<%= will_paginate %>
<ul class="users">
  <% @users.each do |user| %>
    <li>
      <%= gravatar_for user, size: 50 %>
      <span>
        <%= link_to user.name, user %>
        <%= link_to user.email, user %>
      </span>
    </li>
  <% end %>
</ul>
<%= will_paginate %>
